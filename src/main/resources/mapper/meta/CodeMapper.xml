<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="com.meta.mapper.meta.CodeMapper">
    <select id="getGrpData" parameterType="com.meta.dto.TbCodeDto" resultType="com.meta.dto.TbCodeDto">
        SELECT CD, CD_NM
        FROM tb_code
        WHERE GRP_CD = #{grpCd}
        AND   STTS_CD = '1'
        ORDER BY GRP_CD, ORD, CD
    </select>

</mapper>
